guard DynuCredentials: {
	"name": "dynu",
	"key": string
};

guard GlesysCredentials: {
	"name": "glesys",
	"username": string,
	"password": string
};

guard Credentials: DynuCredentials | GlesysCredentials;

guard Provider: {
	"domains": string[],
	"credentials": Credentials
};

guard Certificate: {
	"domains": string[],
	"root": string,
	"account"?: string,
	"key"?: string,
	"cert"?: string
};

guard Options: {
	"providers"?: Provider[],
	"certificates"?: Certificate[]
};
