guard ASN1Integer: ../asn1/Integer;
guard ASN1Node: ../asn1/Node;
guard ASN1Null: ../asn1/Null;
guard ASN1ObjectIdentifier: ../asn1/ObjectIdentifier;
guard ASN1OctetString: ../asn1/OctetString;
guard ASN1Sequence: ../asn1/Sequence;

guard AlgorithmIdentifier: ASN1Sequence & {
	data: [
		ASN1ObjectIdentifier,
		ASN1Node
	]
};

guard AES256CBCAlgorithmIdentifier: AlgorithmIdentifier & {
	data: [
		ASN1ObjectIdentifier & { data: "2.16.840.1.101.3.4.1.42" | "YIZIAWUDBAEq" },
		ASN1OctetString
	]
};

guard HMACSHA256AlgorithmIdentifier: AlgorithmIdentifier & {
	data: [
		ASN1ObjectIdentifier & { data: "1.2.840.113549.2.9" | "KoZIhvcNAgk" },
		ASN1Null
	]
};

guard PBKDF2AlgorithmIdentifier1: AlgorithmIdentifier & {
	data: [
		ASN1ObjectIdentifier & { data: "1.2.840.113549.1.5.12" | "KoZIhvcNAQUM" },
		ASN1Sequence & {
			data: [
				ASN1OctetString | AlgorithmIdentifier,
				ASN1Integer,
				ASN1Integer,
				AlgorithmIdentifier
			]
		}
	]
};

guard PBKDF2AlgorithmIdentifier2: AlgorithmIdentifier & {
	data: [
		ASN1ObjectIdentifier & { data: "1.2.840.113549.1.5.12" | "KoZIhvcNAQUM" },
		ASN1Sequence & {
			data: [
				ASN1OctetString | AlgorithmIdentifier,
				ASN1Integer,
				AlgorithmIdentifier
			]
		}
	]
};

guard PBES2AlgorithmIdentifier: AlgorithmIdentifier & {
	data: [
		ASN1ObjectIdentifier & { data: "1.2.840.113549.1.5.13" | "KoZIhvcNAQUN" },
		ASN1Sequence & {
			data: [
				AlgorithmIdentifier,
				AlgorithmIdentifier
			]
		}
	]
};
