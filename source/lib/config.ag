guard ProviderDynu: {
	"type": "dynu",
	"key": string
};

guard ProviderGlesys: {
	"type": "glesys",
	"account": string,
	"key": string
};

guard Provider: ProviderDynu | ProviderGlesys;

guard Certificate: {
	"hostnames": string[],
	"root"?: string
};

guard Filenames: {
	"account_key"?: string,
	"certificate_key"?: string,
	"certificate"?: string
};

guard Passphrases: {
	"account_pass"?: string,
	"certificate_pass"?: string
};

guard Options: {
	"providers": Provider[],
	"certificates": Certificate[],
	"acme"?: string,
	"monitor"?: boolean,
	"filenames"?: Filenames,
	"passphrases"?: Passphrases
};
