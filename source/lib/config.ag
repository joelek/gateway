guard DynuApi: {
	"name": "dynu",
	"hostname": string,
	"key": string
};

guard GlesysApi: {
	"name": "glesys",
	"hostname": string,
	"account": string,
	"key": string
};

guard Api: DynuApi | GlesysApi;

guard Domain: {
	"hostname": string,
	"api": Api
};

guard Certificate: {
	"domains"?: Domain[],
	"root"?: string,
	"account"?: string,
	"key"?: string,
	"cert"?: string
};

guard Options: {
	"certificates"?: Certificate[]
};
